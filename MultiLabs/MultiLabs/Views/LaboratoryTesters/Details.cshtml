@model MultiLabs.Models.LaboratoryTesters

@{
    ViewData["Title"] = "Técnicos";
}

<h1>Detalhes</h1>

<div>
    <h4>Técnico associado ao laboratório</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Laboratory)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Laboratory.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.UserName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Voltar</a>
</div>
